<h1>All co-works</h1>

<div class="container">

  {{#each coworks}}
  <div class="card" style="width: 18rem;">
    <img class="card-img-top" src="/images/coworkspace.jpg" alt="Card image cap">
    <div class="card-body">
      <h5 class="card-title"></h5>
    </div>
    <ul class="list-group list-group-flush">
      <li class="new1 list-group-item"><strong>{{this.name}}</strong></li>
      <li class="new2 list-group-item">{{this.address}}</li>
      <li class="new3 list-group-item"><strong>{{this.prices}}</strong></li>
      <a href="/cowork-detail/{{this._id}}" class="card-link">More info</a>
      <div class="btn btn-outline-primary">
      <button><a href="/cowork-detail/{{this._id}}" class="card-link">Book now!</a> </button>
      </div>
      {{!-- <button class="card-link btn btn-outline-primary">Book now</button>   --}}
    </ul>
    <div class="card-body">
    </div>
  </div>
{{/each}}

</div>
