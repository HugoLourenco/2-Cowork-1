<h1>All co-works</h1>

<div class="container">

  {{#each coworks}}
  <div class="card" style="width: 18rem;">
    <img class="card-img-top" src="/images/coworkspace.jpg" alt="Card image cap">
    <div class="card-body">
      <h5 class="card-title"></h5>
    </div>
    <ul class="list-group list-group-flush">
      <li class="list-group-item">{{this.name}}</li>
      <li class="list-group-item">{{this.adress.street}}</li>
      <li class="list-group-item">{{this.prices}}</li>
      <a href="#" class="card-link">More info</a>
    </ul>
    <div class="card-body">
    </div>
  </div>
{{/each}}

</div>
